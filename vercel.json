{
  "version": 2,
  "name": "accessibility-first-call-center-poc",
  "builds": [
    {
      "src": "out/**",
      "use": "@vercel/static"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Accessibility-First Call Center System - POC",
    "NEXT_PUBLIC_APP_VERSION": "POC v1.0",
    "NEXT_PUBLIC_DEMO_MODE": "true"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 