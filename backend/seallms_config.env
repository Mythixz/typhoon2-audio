# SeaLLMs Configuration (Simple Setup)
# Copy this to your .env file or set as environment variables

# SeaLLMs API Configuration
SEALLMS_API_URL=https://api.seallms.ai
SEALLMS_API_KEY=your_api_key_here

# Model Configuration
SEALLMS_MODEL_SIZE=1.5B
SEALLMS_DEFAULT_LANGUAGE=th

# Performance Configuration
MAX_TOKENS=500
TEMPERATURE=0.7
REQUEST_TIMEOUT=30

# Fallback Configuration
ENABLE_FALLBACK_TTS=true
ENABLE_FALLBACK_STT=true

# Development Configuration
DEBUG_MODE=true
MOCK_OTP=true
MOCK_SMS=true

# Important: Get your SeaLLMs API key from:
# https://huggingface.co/SeaLLMs
